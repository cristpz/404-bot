const Discord = require("discord.js");

module.exports.run = async (bot, message, args) => {
   let donateembed = new Discord.RichEmbed()
    .setTitle("A thing I created for those people who could spare any amount of cash.")
    .setDescription("Any kind of donation is fully appreciated.")
    .setHeader("https://cdn.discordapp.com/attachments/484337085264822282/485994626679963649/donate.png")
    .addField("Patreon", "https://www.patreon.com/404bot")
    .addField("PayPal", "https://www.paypal.me/dametucosita");
  message.channel.send(donateembed); 
    }

module.exports.help = {
  name: "donate"
}
